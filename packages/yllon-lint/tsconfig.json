{
  "compilerOptions": {
    "outDir": "./lib",
    "target": "ES5",
    "module": "commonjs",
    "esModuleInterop": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "declaration": true,
    "downlevelIteration": true,
    "types": ["node"]
  },
  "include": ["src"]  //src文件夹的ts文件编译打包到lib文件夹下
}
